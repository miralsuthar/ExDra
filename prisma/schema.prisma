generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url = env("DATABASE_URL")
  relationMode = "prisma"
}

model ExcaliState {
    id          String      @id @default(cuid())
    userId      String      @unique
    title       String      @db.VarChar(255)
    createdAt   DateTime    @default(now())
}